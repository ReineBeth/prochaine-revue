{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pr/tuile",
	"version": "0.1.0",
	"title": "Pr Tuile",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"tiles": {
			"type": "array",
			"default": []
		},
		"mode": {
			"type": "string",
			"default": "static"
		},
		"articlesCount": {
			"type": "number",
			"default": 3
		},
		"showAllArticles": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "pr-tuile",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
